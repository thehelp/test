{
  "name": "thehelp-test",
  "version": "3.0.1",
  "main": "src/server/index.js",

  "dependencies": {
    "amdefine": "0.1.x",
    "chai": "1.9.1",
    "lodash": "2.x",
    "sinon": "1.9.1",
    "winston": "0.7.x"
  },
  "devDependencies": {
    "blanket": "1.1.6",
    "grunt": "0.4.5",
    "thehelp-client-project": "0.3.0",
    "thehelp-project": "3.3.1"
  },

  "scripts": {
    "test": "grunt"
  },

  "config": {
    "blanket": {
      "data-cover-only": "src",
      "data-cover-never": "['lib/','node_modules/','test/','src/client']"
    }
  },

  "description": "One-step install for client/server testing tools: chai, mocha, sinon and code coverage via blanket",
  "keywords": [
    "grunt",
    "thehelp",
    "testing",
    "client/server",
    "client",
    "server",
    "blanket",
    "chai",
    "mocha",
    "sinon",
    "code coverage"
  ],

  "author": "Scott Nonnenberg <scott@nonnenberg.com>",
  "license": "MIT",

  "homepage": "https://github.com/thehelp/test",
  "repository": {
    "type": "git",
    "url": "git@github.com:thehelp/test.git"
  },
  "bugs": "https://github.com/thehelp/test/issues"
}
